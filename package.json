{
  "name": "ip-api-static-hosting",
  "version": "1.0.0",
  "description": "Cloudflare Worker with IP API and static site hosting with visitor logging",
  "main": "index.js",
  "scripts": {
    "build-static": "node build-static.js",
    "dev": "wrangler dev --local",
    "deploy": "npm run build-static && wrangler deploy",
    "setup-db": "wrangler d1 execute visitor_logs --local --file=./schema.sql",
    "logs": "wrangler tail",
    "preview": "wrangler dev"
  },
  "keywords": [
    "cloudflare-workers",
    "ip-api",
    "static-hosting",
    "visitor-logging",
    "analytics"
  ],
  "author": "ariedotcodotnz",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^4.33.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ariedotcodotnz/ip.pkgforge.dev"
  }
}